{
	"schemaVersion": 1,
	"id": "block-overlay",
	"version": "${version}",
	"name": "Block Overlay",
	"description": "Customizable block overlay rendering.",
	"authors": [
		"Kulsgam"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/block-overlay/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.kulsgam.BlockOverlay"
		],
		"client": [
			"com.kulsgam.BlockOverlayClient"
		]
	},
